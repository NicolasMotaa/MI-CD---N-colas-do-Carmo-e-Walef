module muxdivisor4x4 (
    output [3:0] Z, // Saída final
    input [3:0] A, // Q
    input [3:0] B, // Resto
    input [3:0] S // Borrow
);

    // Instâncias bit a bit
    muxdivisorbase U0 (Z[0], A[0], B[0], S[0]);
    muxdivisorbase U1 (Z[1], A[1], B[1], S[1]);
    muxdivisorbase U2 (Z[2], A[2], B[2], S[2]);
    muxdivisorbase U3 (Z[3], A[3], B[3], S[3]);

endmodule